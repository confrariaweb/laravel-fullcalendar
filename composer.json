{
    "name": "confrariaweb/laravel-fullcalendar",
    "version": "dev-master",
    "description": "Laravel package to implement the FullCalendar.io",
    "keywords": [
        "fullcalendar",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Rafael Zingano",
            "email": "confrariaweb@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "ConfrariaWeb\\Fullcalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfrariaWeb\\Fullcalendar\\FullcalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "ConfrariaWeb\\Fullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev"
}
